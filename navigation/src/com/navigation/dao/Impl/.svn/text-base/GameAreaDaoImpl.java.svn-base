package com.navigation.dao.Impl;

import java.io.Serializable;

import com.navigation.dao.GameAreaDao;
import com.navigation.dao.base.DaoSupport;
import com.navigation.pojo.GameArea;

/**
 * @author WANGHUI
 * Apr 8, 2011 11:21:36 AM
 */
public class GameAreaDaoImpl extends DaoSupport implements GameAreaDao {

	/**
	 * 创建对象
	 */
	public GameArea save(GameArea user){
		super.save(user);
		return user;
	}
	
	/**
	 * 删除对象
	 */
    public void delete(GameArea record) {
        super.delete(record);
    }
    
    /**
     * 返回对象
     */
	public GameArea get(Serializable id) {
        return super.get(GameArea.class, id);
    }

	/**
	 * 更新对象
	 */
	public void update(GameArea record) {
		super.update(record);
	}


}
