package com.navigation.dao.Impl;

import java.io.Serializable;

import com.navigation.dao.GameDao;
import com.navigation.dao.base.DaoSupport;
import com.navigation.pojo.Game;

/**
 * @author WANGHUI
 * Apr 8, 2011 11:21:36 AM
 */
public class GameDaoImpl extends DaoSupport implements GameDao {

	/**
	 * 创建对象
	 */
	public Game save(Game user){
		super.save(user);
		return user;
	}
	
	/**
	 * 删除对象
	 */
    public void delete(Game record) {
        super.delete(record);
    }
    
    /**
     * 返回对象
     */
	public Game get(Serializable id) {
        return super.get(Game.class, id);
    }

	/**
	 * 更新对象
	 */
	public void update(Game record) {
		super.update(record);
	}


}
