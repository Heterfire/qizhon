package com.navigation.domain.user;

/**
 * @author wenjian
 *
 */
public class UserBean {

    private java.lang.Integer userId;
    private String clientIp;
    
    /* 注册基本信息：登录email和密码 */
    private java.lang.String email;   
    private java.lang.String password;
    private java.lang.String verifyCode;
    //旧密码
    private String oldPassword;

    /* 其它个人资料信息 */
    private java.lang.String uname;
    private java.lang.Short bYear;
    private java.lang.Short bMonth;
    private java.lang.Short bDay;
    private java.lang.String mobile;
    private java.lang.String tel;
    private java.lang.String zip;
    private java.lang.String addr;
    private java.lang.String province;
    private java.lang.String city;
    private java.lang.String district;
    
    
    
    
    
    public java.lang.String getUname() {
        return uname;
    }
    public void setUname(java.lang.String uname) {
        this.uname = uname;
    }
    public java.lang.Short getBYear() {
        return bYear;
    }
    public void setBYear(java.lang.Short year) {
        bYear = year;
    }
    public java.lang.Short getBMonth() {
        return bMonth;
    }
    public void setBMonth(java.lang.Short month) {
        bMonth = month;
    }
    public java.lang.Short getBDay() {
        return bDay;
    }
    public void setBDay(java.lang.Short day) {
        bDay = day;
    }
    public java.lang.String getMobile() {
        return mobile;
    }
    public void setMobile(java.lang.String mobile) {
        this.mobile = mobile;
    }
    public java.lang.String getTel() {
        return tel;
    }
    public void setTel(java.lang.String tel) {
        this.tel = tel;
    }
    public java.lang.String getZip() {
        return zip;
    }
    public void setZip(java.lang.String zip) {
        this.zip = zip;
    }
    public java.lang.String getAddr() {
        return addr;
    }
    public void setAddr(java.lang.String addr) {
        this.addr = addr;
    }
    public java.lang.String getProvince() {
        return province;
    }
    public void setProvince(java.lang.String province) {
        this.province = province;
    }
    public java.lang.String getCity() {
        return city;
    }
    public void setCity(java.lang.String city) {
        this.city = city;
    }
    public java.lang.String getDistrict() {
        return district;
    }
    public void setDistrict(java.lang.String district) {
        this.district = district;
    }
    public String getOldPassword() {
        return oldPassword;
    }
    public void setOldPassword(String oldPassword) {
        this.oldPassword = oldPassword;
    }
    public java.lang.Integer getUserId() {
        return userId;
    }
    public void setUserId(java.lang.Integer userId) {
        this.userId = userId;
    }
    public java.lang.String getVerifyCode() {
        return verifyCode;
    }
    public void setVerifyCode(java.lang.String verifyCode) {
        this.verifyCode = verifyCode;
    }
    public java.lang.String getEmail() {
        return email;
    }
    public void setEmail(java.lang.String email) {
        this.email = email;
    }
    public java.lang.String getPassword() {
        return password;
    }
    public void setPassword(java.lang.String password) {
        this.password = password;
    }
    public String getClientIp() {
        return clientIp;
    }
    public void setClientIp(String clientIp) {
        this.clientIp = clientIp;
    }
    
    
    
}
