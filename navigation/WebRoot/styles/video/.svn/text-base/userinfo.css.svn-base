.ilist .edit input {
    border: 0 none;
    width: 216px;
}
.ilist .edit input:focus {
    border: 0 none;
    outline: 0 none;
}
.ilist .edit a {
    color: #666666;
    font-size: 12px;
}
.ilist .birthday select {
    width: 50px;
}
.ilist .indicator {
    background: url("http://i0.6.cn/3a/3a/j184991302160507.gif") no-repeat scroll 10px center #FFFFFF;
    border: 1px solid #D1D1D1;
    display: none;
    left: 0;
    padding: 4px 0;
    position: absolute;
    text-indent: 40px;
    top: 0;
    width: 100%;
}
.ilist .updateing .indicator {
    display: block;
}
.ilist .updateing p {
    border: medium none;
}
.ilist .updateing dd {
    border: 0 none;
    z-index: auto;
}
.ilist .updateing .value {
    visibility: hidden;
}
.record-mod h2 {
    border-bottom: 1px solid #D1D1D1;
    color: #FF109F;
    font-size: 16px;
    padding: 6px 10px;
}
.record-base .ilist {
    padding: 0 0 0 20px;
}
.record-base dl {
    float: left;
    margin: 0 20px 0 0;
    width: 125px;
}
.manage-title {
    border-bottom: 1px dotted #6A6868;
    color: #333333;
    font-size: 16px;
    margin: 0 3px;
    padding: 8px;
}
.manage-block {
    margin: 0 3px;
    padding: 10px;
    position: relative;
}
.manage-block h2 {
    color: #333333;
    font-size: 14px;
    padding: 10px 0;
}
.manage-block .save {
    clear: both;
    padding: 20px 0;
}
.manage-block .loading {
    display: none;
}
.manage-block-loading .loading {
    display: block;
    height: 330px;
    left: 0;
    position: absolute;
    top: 0;
    width: 100%;
}
.manage-sort {
    background: url("http://i1.6.cn/5a/81/g266701303272891.png") repeat scroll 0 0 transparent;
    float: left;
    height: 216px;
    margin: 0 20px 0 0;
    position: relative;
    width: 535px;
}
.manage-sort li {
    float: left;
    height: 92px;
    overflow: hidden;
    padding: 8px 10px 8px 7px;
    position: relative;
    width: 90px;
}
.manage-sort .pic {
    background: none repeat scroll 0 0 #EFEFEF;
    cursor: move;
    height: 92px;
    overflow: hidden;
    width: 90px;
}
.manage-sort .pic span {
    display: block;
    height: 92px;
    margin: 0 0 0 -20px;
    text-align: center;
    width: 130px;
}
.manage-sort .pic img {
    height: 92px;
}
.manage-sort .close {
    background: url("http://i2.6.cn/95/8d/o443111303279203.png") no-repeat scroll 0 0 transparent;
    cursor: pointer;
    height: 16px;
    position: absolute;
    right: 10px;
    top: 8px;
    width: 15px;
}
.manage-sort-audio {
    background-image: url("http://i0.6.cn/1d/b3/m999881304415468.png");
}
.manage-sort-audio .musicTitle {
    background: none repeat scroll 0 0 #FFFFFF;
    color: #FF0000;
    opacity: 0.5;
}
.manage-sort-audio li {
    background: url("http://i0.6.cn/04/89/w862331304474899.png") no-repeat scroll 0 0 transparent;
    cursor: move;
}
.manage-save {
    padding: 18px;
}
.layout-show {
    border: 0 none;
    margin: 10px auto 0;
    position: relative;
    width: 100%;
}
.show-setting {
    background: url("http://i0.6.cn/3b/0f/x214051304564963.png") no-repeat scroll 5px 3px #FF0098;
    border: 1px solid #FF0098;
    border-radius: 4px 4px 4px 4px;
    display: none;
    float: left;
    left: 6px;
    padding: 1px 6px 1px 26px;
    position: absolute;
    top: 6px;
}
.show-setting a {
    color: #FFFFFF;
    font-size: 14px;
    font-weight: 700;
}
.audioBox {
    position: relative;
}
.audioBox .uploadBar {
    border-bottom: 1px dotted #CCCCCC;
    display: none;
    padding: 10px;
}
.audioBox-wait .mlist .play {
    background: url("http://vr7.6.cn/imges/live/IMGSRC/loading_wb.gif") no-repeat scroll 0 0 transparent;
}
.fans-title {
    border-bottom: 1px dashed #EEEEEE;
    font-size: 16px;
    padding: 10px;
}
.fans-title strong {
    color: #FF109F;
}
