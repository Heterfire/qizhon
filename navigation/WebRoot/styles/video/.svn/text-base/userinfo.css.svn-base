html, body, div, p, h1, h2, h3, h4, blockquote, pre, form, fieldset, ol, ul, li, dl, dd, dt, table, tbody, thead, tfoot, tr, th, td {
    margin: 0;
    padding: 0;
}
html {
    overflow-y: scroll;
}

body {
    background: none repeat scroll 0 0 transparent;
    color: #000000;
    font: 12px/1.5 "Microsoft Yahei",Tahoma,Simsun,Arial,Helvetica,sans-serif;
}
img {
    border: 0 none;
}
button {
    cursor: pointer;
    vertical-align: middle;
}
input, img, select {
    vertical-align: middle;
}
input, textarea {
    background-image: none;
    font-family: "Courier New";
    font-size: inherit;
}
ul, ol {
    list-style: none outside none;
}
table {
    border-collapse: collapse;
}
